	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/share/man/man1/ || $(MKDIR) $(INSTALL_ROOT)/usr/share/man/man1/ 
	-$(INSTALL_FILE) /home/alex/Workstation/GNOME2\ WORKSTATION/wallch-2.1/data/man/wallch.1.gz $(INSTALL_ROOT)/usr/share/man/man1/


uninstall_manfiles:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/share/man/man1/wallch.1.gz
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/share/man/man1/ 


install_imagefiles: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/share/pixmaps/ || $(MKDIR) $(INSTALL_ROOT)/usr/share/pixmaps/ 
	cp data/mime/128/application-x-wallch-project.png /usr/share/icons/hicolor/128x128/mimetypes/; cp data/mime/64/application-x-wallch-project.png /usr/share/icons/hicolor/64x64/mimetypes/; cp data/mime/48/application-x-wallch-project.png /usr/share/icons/hicolor/48x48/mimetypes/; cp data/mime/32/application-x-wallch-project.png /usr/share/icons/hicolor/32x32/mimetypes/; cp data/mime/22/application-x-wallch-project.png /usr/share/icons/hicolor/22x22/mimetypes/; cp data/mime/16/application-x-wallch-project.png /usr/share/icons/hicolor/16x16/mimetypes/; cp data/mime/wallch.xml /usr/share/mime/packages/;
	-$(INSTALL_FILE) /home/alex/Workstation/GNOME2\ WORKSTATION/wallch-2.1/data/pixmap/wallch.png $(INSTALL_ROOT)/usr/share/pixmaps/


uninstall_imagefiles:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/share/pixmaps/wallch.png
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/share/pixmaps/ 


install:  install_docfiles install_shortcutfiles install_helpfiles install_configfiles install_binfiles install_manfiles install_imagefiles  FORCE

uninstall: uninstall_shortcutfiles uninstall_helpfiles uninstall_configfiles uninstall_binfiles uninstall_manfiles uninstall_imagefiles   FORCE

FORCE:

